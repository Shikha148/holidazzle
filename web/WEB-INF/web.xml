<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>RegisterServlet</servlet-name>
        <servlet-class>newpackage.RegisterServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>newpackage.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LogoutServlet</servlet-name>
        <servlet-class>newpackage.LogoutServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AdminLoginServlet</servlet-name>
        <servlet-class>newpackage.AdminLoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddDestinationServlet</servlet-name>
        <servlet-class>newpackage.AddDestinationServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddHotelServlet</servlet-name>
        <servlet-class>newpackage.AddHotelServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddPackageServLet</servlet-name>
        <servlet-class>newpackage.AddPackageServLet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddRoomServlet</servlet-name>
        <servlet-class>newpackage.AddRoomServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FinalOrderServlet</servlet-name>
        <servlet-class>newpackage.FinalOrderServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>RegisterServlet</servlet-name>
        <url-pattern>/RegisterServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/LoginServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LogoutServlet</servlet-name>
        <url-pattern>/LogoutServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AdminLoginServlet</servlet-name>
        <url-pattern>/AdminLoginServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddDestinationServlet</servlet-name>
        <url-pattern>/AddDestinationServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddHotelServlet</servlet-name>
        <url-pattern>/AddHotelServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddPackageServLet</servlet-name>
        <url-pattern>/AddPackageServLet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddRoomServlet</servlet-name>
        <url-pattern>/AddRoomServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FinalOrderServlet</servlet-name>
        <url-pattern>/FinalOrderServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
